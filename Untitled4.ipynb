    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "請輸入正整數 n 的值5\n",
      "5!=120\n"
     ]
    }
   ],
   "source": [
    "total=i=1\n",
    "n=int(input(\"請輸入正整數 n 的值\"))\n",
    "while i<=n:\n",
    "    total*=i\n",
    "    i+=1\n",
    "print(\"%d!=%d\" % (n,total))"
   ]
